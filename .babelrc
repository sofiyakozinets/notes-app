{
	"presets": [
		[
			"@babel/preset-env",
			{
				"debug": false,
				"targets": {
					"browsers": ["last 3 versions"]
				},
				"modules": false
			}
		],
		"@babel/preset-react"
	],
	"plugins": [
		[
			"babel-plugin-styled-components",
			{
				"pure": true,
				"displayName": true
			}
		]
	]
}
